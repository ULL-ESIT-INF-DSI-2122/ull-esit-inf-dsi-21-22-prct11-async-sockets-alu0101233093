de la nota\n * @returns Título y contenido de la nota o mensaje de error en caso de que no se encuentre.\n */\nexport function readNote(user: string, title: string): string{ \n    try {     // Si la nota existe\n\n      accessSync('notes/' + user + '/' + title + '.json', constants.R_OK | constants.W_OK);\n      let data: string = readFileSync('notes/' + user + '/' + title + '.json').toString();  // Carga los datos\n\n      // Imprime los datos\n      return JSON.parse(data).title + '\\n' + printColor(JSON.parse(data).content,JSON.parse(data).color)\n\n    } catch (err) { // Si la nota no existe\n      return chalk.red(\"Note not found\");\n    }\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"b48dcdd7579a31ed896a64b850765e914767a62f","contentHash":"063c3713afdc4c70d305cf767a1ac37ff178a0c5f7c521e32dda41840d72bce7"},"/home/usuario/P11/src/funciones/listNotes.ts":{"path":"/home/usuario/P11/src/funciones/listNotes.ts","statementMap":{"0":{"start":{"line":2,"column":22},"end":{"line":4,"column":1}},"1":{"start":{"line":3,"column":4},"end":{"line":3,"column":62}},"2":{"start":{"line":5,"column":0},"end":{"line":5,"column":62}},"3":{"start":{"line":6,"column":0},"end":{"line":6,"column":27}},"4":{"start":{"line":7,"column":16},"end":{"line":7,"column":49}},"5":{"start":{"line":8,"column":13},"end":{"line":8,"column":26}},"6":{"start":{"line":15,"column":4},"end":{"line":25,"column":5}},"7":{"start":{"line":16,"column":8},"end":{"line":16,"column":89}},"8":{"start":{"line":17,"column":20},"end":{"line":17,"column":58}},"9":{"start":{"line":19,"column":8},"end":{"line":19,"column":32}},"10":{"start":{"line":20,"column":8},"end":{"line":20,"column":72}},"11":{"start":{"line":20,"column":27},"end":{"line":20,"column":70}},"12":{"start":{"line":21,"column":8},"end":{"line":21,"column":22}},"13":{"start":{"line":24,"column":8},"end":{"line":24,"column":53}},"14":{"start":{"line":27,"column":0},"end":{"line":27,"column":30}}},"fnMap":{"0":{"name":"(anon